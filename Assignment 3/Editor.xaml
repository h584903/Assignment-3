<Window x:Class="Assignment_3.Editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment_3"
        mc:Ignorable="d"
        Title="Editor" Height="144" Width="566">
    <Grid Margin="0,0,0,-6">
        <Label Content="Student Id" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="sId" HorizontalAlignment="Left" Margin="10,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="170,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="sName" HorizontalAlignment="Left" Margin="170,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Label Content="Age" HorizontalAlignment="Left" Margin="330,10,0,0" VerticalAlignment="Top"/>
        <TextBox Name="sAge"  HorizontalAlignment="Left" Margin="330,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="150"/>
        <Button Name="bAdd" Content="Add Student" Margin="458,94,0,0" HorizontalAlignment="Left" Width="98" Height="21" VerticalAlignment="Top" Click="bAdd_Click"/>
    </Grid>
</Window>
